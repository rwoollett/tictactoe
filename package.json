{
  "name": "ttt-service",
  "version": "1.0.0",
  "description": "Apollo client/server with GraphQL Nexus schema. Prisma for the stack SQL database",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only src/api/server.ts",
    "start": "ts-node --transpile-only src/api/server.ts",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/graphql/schema.ts",
    "test": "npm run generate:nexus && jest",
    "seeddocker": "ts-node ./src/prisma/seed_docker.ts"
  },
  "keywords": [
    "apollo",
    "nexus",
    "graphql"
  ],
  "author": "Rodney Woollett <rodney.woollett@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@graphql-tools/executor-graphql-ws": "^1.3.1",
    "@graphql-tools/url-loader": "^8.0.14",
    "@prisma/client": "^5.22.0",
    "amqplib": "^0.10.3",
    "braces": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv-cli": "^8.0.0",
    "dset": "^3.1.4",
    "graphql": "^16.8.1",
    "graphql-redis-subscriptions": "^2.7.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.16.0",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "nexus": "^1.3.0",
    "redis": "^4.7.0",
    "ts-node": "^10.9.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^22.10.2",
    "@types/redis": "^4.0.10",
    "babel-jest": "^29.5.0",
    "concurrently": "^8.0.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.1",
    "get-port": "^5.1.1",
    "graphql-request": "^6.0.0",
    "jest": "^29.5.0",
    "nyc": "^15.1.0",
    "prisma": "^5.22.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node ./src/prisma/seed.ts"
  }
}
